[Unit]
Description=Mutimax Interface
After=network.target

[Service]
Type=simple
Environment=NODE_ENV=production
WorkingDirectory=/opt/mutimax-interface/backend
ExecStart=/usr/bin/node /opt/mutimax-interface/backend/server.js
Restart=always
User=www-data
Group=www-data

[Install]
WantedBy=multi-user.target
